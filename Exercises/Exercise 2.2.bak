#reader(lib "htdp-advanced-reader.ss" "lang")((modname |Exercise 2.2|) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #t #t none #f () #f)))
(define (point x y) cons x y)

(define (x-point p) car p)
(define (y-point p) cdr p)

(define (print-point p)
  (newline)
  (display "(")
  (display (x-point p))
  (display ",")
  (display (y-point p))
  (display ")"))
